{
    "_producer_rules": [
        [ [ "collection"  ], [["get", "new_collection_entry"]], {} ]
    ],

    "new_collection_entry": [ "^^", "execute", [[
        [ "get", "__record_entry__" ],
        [ "set_path", [ "^^", "get", "collection_name" ] ],
        [ "attach", [ "^", "work_collection" ] ],
        [ "plant", [
            "_parent_entries", [ "AS^IS", [ "^", "byname", "core_collection" ] ],
            "tags", [ "collection" ],
            "contained_entries", {}
          ] ],
        [ "pluck", "collection_name" ],
        [ "save" ]
    ]] ]
}
