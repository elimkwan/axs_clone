{
    "_producer_rules": [
        [ [ "git_repo", "collection" ], [["clone"]], {
            "tags": ["git_repo", "collection"],
            "newborn_parent_names": [ "git", "core_collection" ],
            "contained_entries": {}
        } ]
    ],

    "_parent_entries": [ [ "^", "byquery", "shell_tool,can_git" ], [ "^", "byname", "entry_creator" ] ],

    "rel_clone_dir": [ "^^", "substitute", "cloned_#{repo_name}#" ],

    "patch_tool_entry": [ "^", "byquery", "shell_tool,can_patch" ],

    "git_url": [ "^^", "execute", [[
        [ "run", [], {
            "cmd_key": "origin_url",
            "repo_path": [ "^^", "get", "repo_path" ],
            "capture_output": true
        } ]
    ]] ],

    "url_prefix": [ "^^", "execute", [[
        [ "get", "git_url" ],
        0,
        [ "func", "os.path.dirname" ]
    ]] ],

    "url": [ "^^", "substitute", "#{url_prefix}#/#{repo_name}#" ],
    "repo_name": [ "^^", "url_2_repo_name" ],

    "repo_path": [ "^^", "get_path" ],

    "current_checkout": [ "^^", "execute", [[
        [ "run", [], {
            "cmd_key": "branch",
            "repo_path": [ "^^", "get", "repo_path" ],
            "capture_output": true
        } ],
        0,
        [ "func", ["ufun.rematch", "^\\*.*\\s(\\S+?)\\)?$"] ]
    ]] ],

    "checkout": [ "^^", "propose_checkout" ],


    "checkout_part": [ "^^", "case", [ [ "^^", "get", ["checkout", ""] ], ["", false, null, "None"], "" ],
                                { "default_value": [ "^^", "func", [ "re.sub", "\\W", "_", [ "^^", "substitute", "_#{checkout}#" ] ] ] }
                     ],
    "patch_part": [ "^^", "case", [ [ "^^", "get", ["patch", ""] ], ["", false, null, "None"], "" ],
                                { "default_value": [ "^^", "func", [ "re.sub", "\\W", "_", [ "^^", "substitute", "_#{patch}#" ] ] ] }
                     ],

    "newborn_entry_tags": [ "git_repo" ],
    "newborn_parent_names": [ "git" ],
    "newborn_name_template": "#{repo_name}##{checkout_part}##{patch_part}#",
    "newborn_entry_param_names": [ "repo_name", "checkout", "submodules", "abs_patch_path", "patch", "contained_files", "clone_options" ]
}
