{
    "_parent_entries": [ [ "^", "byname", "python_script" ] ],
    "_producer_rules": [
        [ [ "program_output", "calendar" ], [["run"]], { "return_saved_record_entry": false } ],
        [ "collection_name" ]
    ],
    
    "python_deps": [ ],

    "first_weekday": 0,
    "month": 10,
    "year": 2022,

    "collection_name": [ "^", "execute", [[
        [ "work_collection" ],
        [ "get_name" ]
    ]] ],
    "collection_object": [ "^", "byquery", ["^^", "substitute", "collection,collection_name=#{collection_name}#" ], { "parent_recursion": true }, [ "collection_name" ] ],

    "return_this_entry": [ "^^", "execute", [[
        [ "get", "collection_object" ],
        [ "attached_entry", [ ["^^", "substitute", "calendar_#{year}#_#{month}#_#{first_weekday}#" ],["^^", "substitute", {
            "_parent_entries": [ "AS^IS", [ "^", "byname", "base_experiment" ] ],
            "tags": [ "program_output", "calendar" ]
         } ] ] ],
        [ "plant", [ "^^", "slice", [
            "year",
            "month",
            "first_weekday",
            "collection_name"
          ], { "plantable": true } ] ],
        [ "save" ]
      ]] ],
     
    "rel_script_path": "generate_calendar.py",
    "script_extra_params": [ "^^", "substitute", "\"#{return_this_entry.input_json_file_path}#\" \"#{return_this_entry.output_json_file_path}#\" " ]
}
